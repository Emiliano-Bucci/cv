{
  "name": "my-garmin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --passWithNoTests --detectOpenHandles --coverage && open ./coverage/lcov-report/index.html",
    "test:pre-push": "NODE_ENV=test jest --passWithNoTests --detectOpenHandles --coverage --watchAll=false && open ./coverage/lcov-report/index.html",
    "lint": "eslint --fix --config .eslintrc.js 'src/**/*.{ts,tsx,js}'",
    "lint:prepush": "eslint --fix --config .eslintrc.prepush.js 'src/**/*.{ts,tsx,js}'",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@next/bundle-analyzer": "^9.5.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/svg-sprite-loader": "^3.9.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "imagemin-svgo": "^8.0.0",
    "jest": "^26.4.2",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^10.4.0",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "prettier": "^2.1.2",
    "svg-sprite-loader": "^5.0.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint:prepush && npm run test:pre-push"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --config .eslintrc.precommit.js"
    ]
  }
}
